<%= form_for @category, html: { multipart: true } do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
    <% @category.errors.full_messages_for(:name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>
 <%= form.file_field :image %>
  <div>
      <%=form.submit %>
  </div>
<% end %>